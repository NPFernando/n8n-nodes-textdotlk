	},
	"repository": {
		"type": "git",
		"url": "https://github.com/NPFernando/n8n-nodes-textdotlk.git"
	},
	"bugs": {
  		"url": "https://github.com/NPFernando/n8n-nodes-textdotlk/issues"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
