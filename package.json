		"credentials": [
			"dist/credentials/TextDotLkApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/TextDotLk/TextDotLk.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
